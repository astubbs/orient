<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Orient DB Server configuration -->
<orient-server>
    <network>
        <protocols>
            <!-- Default registered protocol. It reads commands using the binary protocol. -->
            <protocol implementation="com.orientechnologies.orient.server.network.protocol.binary.ONetworkProtocolBinary" name="binary"/>
        </protocols>
        <listeners>
            <!-- Default listener using the BINARY protocol bound to localhost, port 8000. If the port is busy
                 then it will try to acquire the next one up to the 8010. -->
            <listener protocol="binary" port-range="8000-8010" ip-address="127.0.0.1"/>
        </listeners>
    </network>
    <storages>
        <!-- Default in-memory storage. Data are not saved permanently. -->
        <storage path="memory:temp" name="temp"/>
    </storages>
    <properties/>
</orient-server>
